    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "@vercel/analytics": "^1.1.1",
    "@vercel/kv": "^0.2.1",
    "@vercel/og": "^0.5.7",
    "ai": "^2.2.22",

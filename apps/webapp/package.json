  "devDependencies": {
    "@ecomai/eslint-config": "workspace:^0.2.0",
    "@ecomai/prettier-config": "workspace:^0.1.0",
    "@ecomai/tailwind-config": "workspace:^0.1.0",
    "@ecomai/tsconfig": "workspace:^0.1.0",
